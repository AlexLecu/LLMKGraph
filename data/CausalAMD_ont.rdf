        <rdfs:label>Relation</rdfs:label>
        <rdfs:comment>An n-ary relation between two entities with provenance.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/lecualexandru/ontologies/2024/11/CausalAMD#PUBLICATION">
        <rdfs:label>Publication</rdfs:label>
        <rdfs:comment>A publication from which the relation was derived.</rdfs:comment>
    </owl:Class>

