        <rdfs:range rdf:resource="http://www.semanticweb.org/lecualexandru/ontologies/2024/11/CausalAMD#Entity"/>
    </owl:ObjectProperty>


    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Relation-Specific Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

