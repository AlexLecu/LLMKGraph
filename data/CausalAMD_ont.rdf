    </owl:Class>

    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Relation-Specific Object Properties with PROV-O Integration
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- Define the relation_subject property -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/lecualexandru/ontologies/2024/11/CausalAMD#relation_subject">
        <rdfs:label>Relation Subject</rdfs:label>
        <rdfs:comment>The subject entity in the relation.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lecualexandru/ontologies/2024/11/CausalAMD#RELATION"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/lecualexandru/ontologies/2024/11/CausalAMD#Entity"/>
